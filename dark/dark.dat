#!/bin/zsh

# disable OUT_DAT
OUT_DAT='FALSE'

COLOR_SCHEME_INI=(
'#000000' \
'#ffffff'
)

CREF_IN='#000000'
CREF_OUT='#A8A8A8'

# paths for images to colorize
PNG_SUBDIRS=()
